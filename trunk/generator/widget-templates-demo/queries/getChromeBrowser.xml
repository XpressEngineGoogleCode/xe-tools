<query id="getChromeBrowser" action="select">
    <tables>
	<table name="counter_log" />
    </tables>
    <columns>
	<column name="COUNT(*)" />
    </columns>
    <conditions>
	<condition operation="like" column="user_agent" default="% Chrome/_%" notnull="notnull" />
    </conditions>
</query>
