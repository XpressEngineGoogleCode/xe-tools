<?xml version="1.0" encoding="UTF-8"?>
<templates>
	<parameters>
		<parameter id="name" description="Module Name" option_name="name" variable_name="$PARAM_MODULE_NAME" required="true"/>
		<parameter id="description" description="Module Description" option_name="desc" variable_name="$PARAM_MODULE_DESCRIPTION" required="false"/>
		<parameter id="version" description="Module Version" option_name="vers" variable_name="$PARAM_MODULE_VERSION" required="false"/>
		<parameter id="date" description="Module Date" option_name="date" variable_name="$PARAM_MODULE_DATE" required="false"/>
		<parameter id="category" description="Module Category" option_name="cat" variable_name="$PARAM_MODULE_CATEGORY" required="false"/>
		<parameter id="author_name" description="Author Name" option_name="author-name" variable_name="$PARAM_MODULE_AUTHOR_NAME" required="false"/>
		<parameter id="author_url" description="Author URL" option_name="author-url" variable_name="$PARAM_MODULE_AUTHOR_URL" required="false"/>
		<parameter id="author_email" description="Author Email" option_name="author-email" variable_name="$PARAM_MODULE_AUTHOR_EMAIL_ADDRESS" required="false"/>
		<parameter id="about" description="Module About" option_name="about" variable_name="$PARAM_MODULE_ABOUT" required="false"/>
		<parameter id="delete_warning" description="Module Delete Warning Message" option_name="del-warn-msg" variable_name="$PARAM_MODULE_DELETE_WARNING_MSG" required="false"/>
		<parameter id="nameuc" alias="name" transform="ucword" variable_name="$PARAM_MODULE_NAME_UC" required="false"/>
	</parameters>
	<folders>
		<folder relative_path="./conf"/>
		<folder relative_path="./lang"/>
		<folder relative_path="./m.skins"/>
		<folder relative_path="./queries"/>
		<folder relative_path="./ruleset"/>
		<folder relative_path="./schema"/>
		<folder relative_path="./skins"/>
		<folder relative_path="./skins/default"/>
		<folder relative_path="./skins/default/css"/>
		<folder relative_path="./skins/default/filter"/>
		<folder relative_path="./skins/default/img"/>
		<folder relative_path="./skins/default/js"/>
		<folder relative_path="./tpl"/>
		<folder relative_path="./tpl/filter"/>
		<folder relative_path="./tpl/icons"/>
		<folder relative_path="./tpl/images"/>
		<folder relative_path="./tpl/js"/>
	</folders>
	<files>
		<file relative_path="./conf/info.xml">
			<param id="name"/>
			<param id="description"/>
			<param id="version"/>
			<param id="date"/>
			<param id="category"/>
			<param id="author_name"/>
			<param id="author_url"/>
			<param id="author_email"/>
		</file>
		<file relative_path="./conf/module.xml">
			<param id="nameuc"/>
			<param id="name"/>
		</file>
		<file relative_path="./lang/lang.xml">
			<param id="nameuc"/>
			<param id="name"/>
			<param id="about"/>
			<param id="delete_warning"/>
		</file>
		<file relative_path="./queries/gettemplateList.xml" replace_file_name_with_param_id="nameuc">
			<param id="nameuc"/>
			<param id="name"/>
		</file>
		<file relative_path="./ruleset/deletetemplate.xml" replace_file_name_with_param_id="nameuc">
		</file>
		<file relative_path="./ruleset/inserttemplate.xml" replace_file_name_with_param_id="nameuc">
			<param id="name"/>
		</file>
		<file relative_path="./ruleset/updatetemplate.xml" replace_file_name_with_param_id="nameuc">
			<param id="name"/>
		</file>
		<file relative_path="./skins/default/skin.xml">
			<param id="name"/>
			<param id="author_name"/>
			<param id="author_url"/>
			<param id="author_email"/>
		</file>
		<file relative_path="./tpl/js/template_admin.js" replace_file_name_with_param_id="name">
			<param id="name"/>			
			<param id="author_name"/>
			<param id="author_email"/>
		</file>
		<file relative_path="./tpl/addition_setup.html">
		</file>
		<file relative_path="./tpl/grant_list.html">
		</file>
		<file relative_path="./tpl/header.html">
			<param id="nameuc"/>
			<param id="name"/>
		</file>
		<file relative_path="./tpl/index.html">
			<param id="nameuc"/>
			<param id="name"/>
		</file>
		<file relative_path="./tpl/template_delete.html" replace_file_name_with_param_id="name">
			<param id="nameuc"/>
			<param id="name"/>
		</file>
		<file relative_path="./tpl/template_info.html" replace_file_name_with_param_id="name">
			<param id="nameuc"/>
			<param id="name"/>
		</file>
		<file relative_path="./tpl/template_insert.html" replace_file_name_with_param_id="name">
			<param id="nameuc"/>
			<param id="name"/>
		</file>
		<file relative_path="./template.admin.controller.php" replace_file_name_with_param_id="name">
			<param id="nameuc"/>
			<param id="name"/>
			<param id="author_name"/>
			<param id="author_email"/>
		</file>
		<file relative_path="./template.admin.model.php" replace_file_name_with_param_id="name">
			<param id="name"/>
			<param id="author_name"/>
			<param id="author_email"/>
		</file>
		<file relative_path="./template.admin.view.php" replace_file_name_with_param_id="name">
			<param id="nameuc"/>
			<param id="name"/>
			<param id="author_name"/>
			<param id="author_email"/>
		</file>
		<file relative_path="./template.class.php" replace_file_name_with_param_id="name">
			<param id="name"/>
			<param id="author_name"/>
			<param id="author_email"/>
		</file>
		<file relative_path="./template.controller.php" replace_file_name_with_param_id="name">
			<param id="name"/>
			<param id="author_name"/>
			<param id="author_email"/>
		</file>
		<file relative_path="./template.item.php" replace_file_name_with_param_id="name">
			<param id="name"/>
			<param id="author_name"/>
			<param id="author_email"/>
		</file>
		<file relative_path="./template.mobile.php" replace_file_name_with_param_id="name">
			<param id="name"/>
			<param id="author_name"/>
			<param id="author_email"/>
		</file>
		<file relative_path="./template.model.php" replace_file_name_with_param_id="name">
			<param id="name"/>
			<param id="author_name"/>
			<param id="author_email"/>
		</file>
		<file relative_path="./template.view.php" replace_file_name_with_param_id="name">
			<param id="name"/>
			<param id="author_name"/>
			<param id="author_email"/>
		</file>
	</files>
</templates>